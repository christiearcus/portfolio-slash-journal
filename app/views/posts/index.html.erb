  <h1>Journal</h1>
  <%= form_tag(posts_path, :method => "get", id: "search-form") do %>
    <%= text_field_tag :search, params[:search], placeholder: "Search Posts" %>
    <%= submit_tag "Search" %>
  <% end %>
  
  <% @posts.each do |post| %>
    <h2><%= link_to post.title, post %></h2>
  <% end %>
  <% if logged_in? %>
  <%= link_to "New", new_post_path %>
  <% end %>
